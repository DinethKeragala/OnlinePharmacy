{
  "name": "online-pharmacy-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for Online Pharmacy application using Selenium WebDriver",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:auth": "jest --testPathPattern=auth",
    "test:products": "jest --testPathPattern=products"
  },
  "keywords": [
    "selenium",
    "webdriver",
    "e2e",
    "testing",
    "pharmacy"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "chromedriver": "^140.0.3",
    "jest": "^29.7.0",
    "selenium-webdriver": "^4.15.0"
  },
  "jest": {
    "testTimeout": 30000,
    "setupFilesAfterEnv": [
      "<rootDir>/utils/test-setup.js"
    ],
    "testMatch": [
      "<rootDir>/e2e/**/*.test.js"
    ]
  }
}
